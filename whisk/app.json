{
  "expo": {
    "name": "Whisk",
    "slug": "whisk",
    "description": "React Native app that uses Redux and Firebase, and consumes the Google Places and Yelp APIs.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/whisk.png",
    "splash": {
      "image": "./assets/images/whisk.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/whisk.png"
    }
  }
}
